{
  "name": "WebApp",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "ci": "npm run install && npm run lint && npm run build:prod",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "npm run build -- --configuration=production",
    "build:ssr": "npm run build -- --app=ssr --output-hashing=media",
    "precise-commits": "precise-commits",
    "precommit": "npm run precise-commits",
    "test": "ng test",
    "lint": "ng lint",
    "docs": "compodoc -p tsconfig.json -s --theme material"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run build:prod"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "7.2.14",
